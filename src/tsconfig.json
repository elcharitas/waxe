{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "../lib",
    "strict": true,
    "strictNullChecks": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "moduleResolution": "node"
  }
}